{
  "name": "flash-sport",
  "homepage": "https://helloajmal007-commits.github.io/livesocer.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate --schema app/admin/prisma/schema.prisma && next build",
    "start": "node server.js",
    "lint": "next lint",
    "postinstall": "prisma generate --schema app/admin/prisma/schema.prisma",
    "clean:cache": "rimraf .gh-pages-cache node_modules/.cache/gh-pages",
    "deploy:flash": "npm run clean:cache && cross-env NEXT_OUTPUT=export NEXT_PUBLIC_REPO_NAME=flash-sport next build && node deploy-manual.js https://github.com/Muhammedbeig/flash-sport.git gh-pages",
    "deploy:live": "npm run clean:cache && cross-env NEXT_OUTPUT=export NEXT_PUBLIC_REPO_NAME=livesocer.com next build && node deploy-manual.js https://github.com/helloajmal007-commits/livesocer.com.git gh-pages"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.1",
    "jose": "^6.1.3",
    "lucide-react": "^0.562.0",
    "next": "16.0.10",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.4",
    "gh-pages": "^6.3.0",
    "prisma": "^6.15.0",
    "rimraf": "^6.1.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
